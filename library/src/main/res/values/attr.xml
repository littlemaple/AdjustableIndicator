<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndicatorView">
        <attr name="shape" format="enum">
            <enum name="circle" value="0" />
            <enum name="triangle" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="line_width" format="dimension" />
        <attr name="angle" format="float" />
        <attr name="size" format="dimension" />
        <attr name="scale" format="float" />
        <attr name="each_padding" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="current" format="integer" />
        <attr name="stroke_color" format="color" />
        <attr name="filled_color" format="color" />
        <attr name="mode" format="enum">
            <enum name="progress" value="0" />
            <enum name="select" value="1" />
        </attr>
    </declare-styleable>
</resources>